#include <stdio.h>

int main() 

{

	double a, b;

 	printf("Enter a: ");

 	scanf("%lf", &a);

 	printf("Enter b: ");

 	scanf("%lf", &b);



 // swapping of two number without using a temp variable by Sri_Krishna



 // a = (first_a - first_b)

 a = a - b;  



 // b = (first_a - first_b) + first_b = first_a

 b = a + b;



 // a = first_a - (first_a - first_b) = first_b

 a = b - a;



 printf("After swapping, a = %.2lf\n", a);

 printf("After swapping, b = %.2lf", b);



 return 0;

}

